{
  "name": "coc-fzf-preview",
  "version": "0.4.9",
  "description": "Dark powered fzf plugin",
  "author": "yuki-yano",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:yuki-ycino/fzf-preview.vim"
  },
  "main": "./lib/index.js",
  "files": [
    "lib",
    "autoload",
    "plugin",
    "bin"
  ],
  "keywords": [
    "coc.nvim",
    "fzf"
  ],
  "engines": {
    "coc": "^0.0.77"
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:remote": "yarn run clean && yarn run webpack --config webpack.remote.js",
    "build:coc": "yarn run clean && yarn run webpack --config webpack.coc.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:remote": "yarn run clean && yarn run webpack --watch --config webpack.remote.js",
    "dev:coc": "yarn run clean && yarn run webpack --watch --config webpack.coc.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "cli": "ts-node --files -r tsconfig-paths/register",
    "clean": "rimraf lib rplugin/node/fzf-preview.vim/index.js",
    "postinstall": "yarn run build:remote"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "ansi-escape-sequences": "^5.1.2",
    "camelcase": "^6.0.0",
    "coc.nvim": "^0.0.77",
    "lodash": "^4.17.19",
    "neovim": "^4.9.0",
    "printf": "^0.5.3",
    "redux": "^4.0.5",
    "strip-ansi": "^6.0.0",
    "type-fest": "^0.16.0",
    "uuid": "^8.2.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/ansi-escape-sequences": "^4.0.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.20",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.16.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.0.9"
  }
}
